{
	"name": "aimeos/aimeos-core",
	"description": "Full-featured e-commerce framework for high performance web shops",
	"homepage": "https://aimeos.org/",
	"license": "LGPL-3.0",
	"keywords": ["e-commerce", "shop", "portal", "market place"],
	"support": {
		"source": "https://github.com/aimeos/aimeos-core",
		"issues": "https://github.com/aimeos/aimeos-core/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-pcre": "*",
		"ext-PDO": "*",
		"ext-pdo_mysql": "*",
		"ext-zip": "*",
		"psr/http-message": "~1.0"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master",
		"squizlabs/php_codesniffer": "~1.0",
		"phpunit/phpunit": "~3.7||~4.0||~5.0",
		"phing/phing": "~2.4"
	},
	"suggest": {
		"apigen/apigen": "Required for generating the API documentation"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\MW\\": "lib/mwlib/src/MW/",
			"Aimeos\\MShop\\": "lib/mshoplib/src/MShop/",
			"Aimeos\\MAdmin\\": "lib/mshoplib/src/MAdmin/",
			"Aimeos\\Controller\\Common\\": "controller/common/src/Controller/Common/",
			"Aimeos\\Controller\\Frontend\\": "controller/frontend/src/Controller/Frontend/",
			"Aimeos\\Controller\\JsonAdm\\": "controller/jobs/src/Controller/JsonAdm/",
			"Aimeos\\Controller\\ExtJS\\": "controller/extjs/src/Controller/ExtJS/",
			"Aimeos\\Controller\\Jobs\\": "controller/jobs/src/Controller/Jobs/",
			"Aimeos\\Client\\Html\\": "client/html/src/Client/Html/",
			"Aimeos\\Admin\\JQAdm\\": "admin/jqadm/src/Admin/JQAdm/",
			"Aimeos\\": "."
		},
		"classmap": [
			"lib/mwlib/src",
			"lib/mshoplib/src",
			"controller/common/src",
			"controller/frontend/src",
			"controller/jsonadm/src",
			"controller/extjs/src",
			"controller/jobs/src",
			"client/html/src",
			"admin/jqadm/src",
			"Bootstrap.php"
		]
	}
}

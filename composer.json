{
	"name": "aimeos/aimeos-core",
	"description": "Full-featured e-commerce framework for high performance web shops",
	"homepage": "https://aimeos.org/",
	"license": "LGPL-3.0",
	"keywords": ["e-commerce", "shop", "portal", "market place"],
	"support": {
		"source": "https://github.com/aimeos/aimeos-core",
		"issues": "https://github.com/aimeos/aimeos-core/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.2.0",
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-pcre": "*",
		"ext-PDO": "*",
		"ext-pdo_mysql": "*",
		"ext-zip": "*",
		"psr/http-message": "~1.0",
		"composer/installers": "~1.0",
		"laravel/framework": "~5.0",
		"league/oauth2-client": "~1.0",
		"monolog/monolog": "~1.0",
		"omnipay/authorizenet": "~2.0",
		"omnipay/cardsave": "~2.0",
		"omnipay/mollie": "~2.0",
		"omnipay/sagepay": "~2.0",
		"omnipay/stripe": "~2.0",
		"omnipay/dummy": "~2.0",
		"phpoffice/phpexcel": "1.8.0",
		"predis/predis": "~1.0",
		"swiftmailer/swiftmailer": "~5.0",
		"symfony/symfony": "~2.4",
		"typo3/flow": "~3.0",
		"zendframework/zend-config": "~2.5",
		"zendframework/zend-log": "~2.5",
		"zendframework/zend-mail": "~2.5",
		"zendframework/zend-mvc": "~2.5",
		"zendframework/zend-i18n": "~2.5",
		"zendframework/zend-servicemanager": "~2.5",
		"league/flysystem": "~1.0",
		"league/flysystem-memory": "~1.0", 
		"league/flysystem-aws-s3-v3": "~1.0", 
		"league/flysystem-copy": "~1.0", 
		"league/flysystem-dropbox": "~1.0", 
		"league/flysystem-gridfs": "~1.0", 
		"league/flysystem-rackspace": "~1.0", 
		"league/flysystem-replicate-adapter": "~1.0", 
		"league/flysystem-sftp": "~1.0", 
		"league/flysystem-webdav": "~1.0", 
		"league/flysystem-ziparchive": "~1.0",
		"league/flysystem-phpcr": "~1.0", 
		"jackalope/jackalope-doctrine-dbal": "~1.0"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master",
		"squizlabs/php_codesniffer": "1.*",
		"phpunit/phpunit": "3.7.*",
		"phing/phing": "2.4.*",
		"tedivm/jshrink": "~1.0"
	},
	"suggest": {
		"phpdocumentor/phpdocumentor": "Required for generating the API documentation"
	},
	"autoload": {
		"psr-0": {
			"MShop": "lib/mshoplib/src",
			"MAdmin": "lib/mshoplib/src",
			"MW": "lib/mwlib/src",
			"Client_Html": "client/html/src",
			"Controller_Common": "controller/common/src",
			"Controller_Frontend": "controller/frontend/src",
			"Controller_ExtJS": "controller/extjs/src",
			"Controller_Jobs": "controller/jobs/src",
			"": ["."]
		},
		"classmap": [
			"lib/mshoplib/src",
			"lib/mwlib/src",
			"client/html/src",
			"controller/common/src",
			"controller/frontend/src",
			"controller/extjs/src",
			"controller/jobs/src",
			"Bootstrap.php"
		]
	}
}
